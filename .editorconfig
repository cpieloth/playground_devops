# Coding style with EditorConfig
# Plugin for your editor/IDE on http://EditorConfig.org

# top-most EditorConfig file
root = true

# General config
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[Dockerfile]
indent_style = space

[Vagrantfile]
indent_style = space
indent_size = 2

[*.yml]
indent_style = space
indent_size = 2
